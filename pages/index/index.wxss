/**index.wxss**/
page{
    display: flex;
    flex-direction: column;
    height: 100%;
}
/* 头部tab切换 */
.nav-tabs{
    width: 100%;
    height: 75rpx;
    display: flex;
    background-color: #ffffff;
}
.default{
    line-height:75rpx;
    text-align:center;
    flex:1;
    border-bottom:1px solid #eee;
    color:#000;
    font-weight:bold;
    font-size:28rpx;
}
.active{
    line-height:75rpx;
    text-align:center;
    color:#fc5558;
    flex:1;
    border-bottom:1px solid red;
    font-weight:bold;
    font-size:28rpx;
}
/* 下方列表 */
.tab-content {
    flex: 1;
}
.scroll-view {
    height: 100%;
}
.scroll-view-loading {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
/* loading */
.loading-text {
    color: #cccccc;
    font-size: 80rpx;
    position: relative;
    overflow: hidden;
}
.loading-hl {
    background-color: rgba(246,246,246,.5);
    /* background-color: #000; */
    width: 30rpx;
    height: 100%;
    position: absolute;
    top: 0;
    left:40rpx;
    transform: skew(-20deg,-20deg);
    animation:mymove 1s linear infinite;
}

@keyframes mymove
{
    0% {left:0px;}
    100% {left: 100%;}
}
.loading-more {
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    color: #888888;
    font-size: 28rpx;
    margin: 20rpx 0rpx;
}
/* 下方列表 */
.list-item {
    background-color: #ffffff;
    padding: 20rpx;
    border-radius: 10rpx;
    margin:20rpx;
}
.item-title {
    font-size: 40rpx;
    margin-bottom: 5rpx;
}
.item-content {
    font-size: 32rpx;
}
.item-click {
    box-shadow: 0px 0px 10px #aaaaaa;
}
.item-play {
    box-shadow: 0px 0px 10px #aaaaaa;
}
/* 播放标 */
.play {
    width: 120rpx;
    height: 120rpx;
    position: absolute;
    right: 60rpx;
    bottom: 150rpx;
    background-size: cover;
    border-radius: 50%;
    z-index: 1;
}
.play-img {
    background-image: url('data:image/png;base64,');
}
.pause-img {
    background-image: url('data:image/png;base64,');
}
.play-click {
    box-shadow: 0px 0px 10px #888888;
}